{
	"name": "react-router-typesafe",
	"version": "1.0.0",
	"description": "type safe patches of react-router-dom",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.mjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"test": "vitest",
		"release": "pnpm build && changeset publish"
	},
	"keywords": [
		"react",
		"react-router",
		"react-router-dom",
		"remix",
		"remix-router"
	],
	"author": "fredericoo",
	"license": "ISC",
	"peerDependencies": {
		"react": ">= 17",
		"react-router-dom": ">= 6.4.0",
		"typescript": ">= 4.9"
	},
	"devDependencies": {
		"@changesets/cli": "^2.26.2",
		"eslint": "^8.45.0",
		"prettier": "^3.0.0",
		"tsup": "^7.1.0",
		"typescript": "^5.1.6",
		"vitest": "^0.33.0"
	}
}
